{
	"IDX.aI.enableInlineCompletion": true,
	"IDX.aI.enableCodebaseIndexing": true,
	"prettier.printWidth": 250,
	"workbench.tree.indent": 20,
	"editor.detectIndentation": false,
	"editor.fontFamily": "Monaspace Neon Var, Fira Code, Monaco, monospace",
	"editor.formatOnSave": true,
	"editor.insertSpaces": false,
	"editor.tabSize": 4,
	"editor.fontSize": 14,
	"editor.lineHeight": 1.5,
	"editor.letterSpacing": -0.5,
	"debug.console.fontSize": 10,
	"workbench.preferredDarkColorTheme": "GitHub Dark",
	"workbench.preferredLightColorTheme": "GitHub Dark",
	"workbench.iconTheme": "monospace",
	"window.autoDetectColorScheme": true,
	"css.lint.unknownAtRules": "ignore",
	"[css]": {
		"editor.defaultFormatter": "bradlc.vscode-tailwindcss"
	}
}