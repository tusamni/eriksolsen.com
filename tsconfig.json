{
    "compilerOptions": {
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "baseUrl": ".",
        "paths": {
            "@/assets/*": [
                "src/assets/*?url"
            ],
            "@/components/*": [
                "src/components/*.astro"
            ],
            "@/layouts/*": [
                "src/layouts/*.astro"
            ],
            "@/functions/*": [
                "src/functions/*.js"
            ],
            "@/data/*": [
                "src/data/*.json"
            ],
            "@/public/*": [
                "public/*"
            ],
            "@/src/*": [
                "src/*"
            ],
            "@/shoots/*": [
                "public/images/shoots/*"
            ]
        },
        "types": [
            "@astrojs/image/client"
        ]
    }
}